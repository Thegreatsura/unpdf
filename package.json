{
  "name": "unpdf",
  "type": "module",
  "version": "0.5.4",
  "packageManager": "pnpm@8.6.12",
  "description": "Utilities to work with PDFs, like extracting text",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/unpdf#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/unpdf.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/unpdf/issues"
  },
  "keywords": [
    "pdf",
    "parse"
  ],
  "sideEffects": false,
  "exports": {
    "browser": "./dist/index.web.mjs",
    "bun": "./dist/index.worker.mjs",
    "deno": "./dist/index.worker.mjs",
    "edge-light": "./dist/index.worker.mjs",
    "edge-routine": "./dist/index.worker.mjs",
    "lagon": "./dist/index.worker.mjs",
    "netlify": "./dist/index.worker.mjs",
    "react-native": "./dist/index.worker.mjs",
    "wintercg": "./dist/index.worker.mjs",
    "worker": "./dist/index.worker.mjs",
    "workerd": "./dist/index.worker.mjs",
    "node": {
      "types": "./dist/index.node.d.ts",
      "import": "./dist/index.node.mjs",
      "require": "./dist/index.node.cjs"
    },
    "types": "./dist/index.web.d.ts",
    "import": "./dist/index.web.mjs",
    "require": "./dist/index.web.cjs"
  },
  "main": "./dist/index.node.cjs",
  "module": "./dist/index.web.mjs",
  "types": "./dist/index.web.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "tsx test/index.ts",
    "test:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "pdfjs-dist": "^3.9.179"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/node": "^20.5.6",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "pdfjs-dist": "^3.9.179",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0"
  }
}
